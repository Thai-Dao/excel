{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "process-Excel",
            "args": [
                "process-Excel",
                "-f=(Name).xlsx",
                "-d=(Name)"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/main.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "compare-yaml",
            "args": [
                "compare-yaml",
                "--d1=(Name)",
                "--d2=(Name)"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/main.js"
        }
    ]
}